<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.1">
<!-- OpenMarket/Xcelerate/AssetType/CSElement/CheckDelete
-
- INPUT
-
- OUTPUT
-  Variable.candelete   (true/false)
-  Variable.DeleteFailedReason
-->

<SETVAR NAME="candelete" VALUE="true"/>


<SELECTTO FROM="SiteCatalog" LIST="sitecatalogentries" WHAT="*" WHERE="rootelement"/>
<IF COND="IsList.sitecatalogentries=true">
    <THEN>
        <if COND="sitecatalogentries.#numRows!=0">
            <then>
                <SETVAR NAME="candelete" VALUE="false"/>
            </then>
        </if>
    </THEN>
</IF>

<SETVAR NAME="errno" VALUE="0"/>

</FTCS> 

