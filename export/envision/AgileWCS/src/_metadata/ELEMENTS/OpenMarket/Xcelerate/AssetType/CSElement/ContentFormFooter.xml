<?xml version="1.0" ?>
<!DOCTYPE FTCS SYSTEM "futuretense_cs.dtd">
<FTCS Version="1.2">
<!-- OpenMarket/Xcelerate/AssetType/CSElement/ContentFormFooter
-
- INPUT
-
- OUTPUT
-
-->
<IF COND="Variables.updatetype=setformdefaults">
<THEN>
    <SETVAR NAME="buttonSrc" VALUE="save.gif"/>
</THEN>
<ELSE>
    <SETVAR NAME="buttonSrc" VALUE="save_changes.gif"/>
</ELSE>
</IF>

	<callelement NAME="OpenMarket/Xcelerate/UIFramework/Util/FooterBar"/>

<tr>
    <td align="left">
        <XLAT.LOOKUP KEY="dvin/AT/Common/CancelChanges" VARNAME="_CancelChanges_" ESCAPE="true"/>
        <XLAT.LOOKUP KEY="dvin/UI/Cancel" VARNAME="_CancelChanges_ALT"/>
        <A HREF="javascript:void(0)" onclick="return SetCancelFlag()" onmouseover="window.status='Variables._CancelChanges_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._CancelChanges_">
					<IMG src="Variables.cs_imagedir/graphics/SessionVariables.locale/button/content/images/cancel.gif" ALT="Variables._CancelChanges_ALT" BORDER="0" VSPACE="10" REPLACEALL="Variables.cs_imagedir,SessionVariables.locale,Variables._CancelChanges_ALT"/>
        </A>
        <IMG height="1" width="10" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
        <XLAT.LOOKUP KEY="dvin/AT/Common/SaveChanges" VARNAME="_SaveChanges_" ESCAPE="true"/>
        <XLAT.LOOKUP KEY="dvin/UI/Save" VARNAME="_SaveChanges_ALT"/>
        <A HREF="javascript:void(0)" onclick="return checkfields();" onmouseover="window.status='Variables._SaveChanges_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables._SaveChanges_,Variables.namestring">
          <IMG src="Variables.cs_imagedir/graphics/SessionVariables.locale/button/content/images/Variables.buttonSrc" ALT="Variables._SaveChanges_ALT" VSPACE="10" BORDER="0" REPLACEALL="Variables.cs_imagedir,SessionVariables.locale,Variables.buttonSrc,Variables._SaveChanges_ALT"/>
        </A>
				<IF COND="IsVariable.NoContinue=false">
					<THEN>
						<IMG height="1" width="10" src="Variables.cs_imagedir/graphics/common/screen/dotclear.gif" REPLACEALL="Variables.cs_imagedir"/>
						<XLAT.LOOKUP KEY="dvin/AT/Common/Continue" VARNAME="_Continue_" ESCAPE="true"/>
						<XLAT.LOOKUP KEY="dvin/AT/Common/Continue" VARNAME="_Continue_ALT"/>
						<A HREF="javascript:void(0)" onclick="return FixpagenameWithCheck('Variables.ThisPage','ContentFormVariables.ContinueTo');" onmouseover="window.status='Variables._Continue_';return true;" onmouseout="window.status='';return true;" REPLACEALL="Variables.ContinueTo,Variables.ThisPage,Variables._Continue_">
							<IMG src="Variables.cs_imagedir/graphics/SessionVariables.locale/button/content/images/continue.gif" ALT="Variables._Continue_ALT" BORDER="0" VSPACE="10" REPLACEALL="Variables.cs_imagedir,SessionVariables.locale,Variables._Continue_ALT"/>
						</A>
					</THEN>
				</IF>
    </td>
</tr>


</FTCS>